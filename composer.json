{
    "name": "olivier127/rbac-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PhpRabcBundle allow to use RBAC control access for symfony project",
    "keywords": [
        "RBAC", "Security", "Access-Control", "Role", "phprbac.net", "owasp", "symfony", "bundle", "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Fouache",
            "email": "olivier.fouache@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "webmozart/assert": "^1.10",
        "doctrine/annotations": "^2.0",
        "symfony/security-bundle": "^6.4 || ^7.0",
        "symfony/orm-pack": "^2.4",
        "symfony/twig-pack": "^1.0"
    },
    "autoload": {
        "psr-4": { "PhpRbacBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\PhpRbacBundle\\": "tests/" }
    }
}
